\color inherit

\begin_inset Formula 
\[
\langle\pi_{k},x\pi_{k-1}\rangle=\langle\pi_{k},\pi_{k}+\sum_{i=0}^{k-1}c_{i}\pi_{i}\rangle=\left\langle \pi_{k},\pi_{k}\right\rangle +\sum_{i=0}^{k-1}c_{i}\left\langle \pi_{k},\pi_{i}\right\rangle =\langle\pi_{k},\pi_{k}\rangle
\]

\end_inset


\end_layout

