\begin_layout Frame
\begin_inset Formula 
\[
\langle x\pi_{k},\pi_{k-1}\rangle=\langle\pi_{k},x\pi_{k-1}\rangle
\]

\end_inset


\family roman
\series medium
\shape up
\size normal
\emph off
\nospellcheck off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\pi_{k-1}$
\end_inset

 je monický 
\begin_inset Formula $\Longrightarrow x\pi_{k-1}$
\end_inset

 je monický
\end_layout

\begin_layout Frame

\family roman
\series medium
\shape up
\size normal
\emph off
\nospellcheck off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\begin{eqnarray*}
\exists p_{k-1}\in P_{k-1}:x\pi_{k-1} & = & x^{k}+p_{k-1}\\
\exists q_{k-1}\in P_{k-1}:\pi_{k} & = & x^{k}+q_{k-1}
\end{eqnarray*}

\end_inset


\begin_inset Formula 
\[
x\pi_{k-1}-\pi_{k}=p_{k-1}-q_{k-1}=r_{k-1}\in P_{k-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Answer
\end_layout

\end_inset


\end_layout

\begin_layout Frame

\family roman
\series medium
\shape up
\size normal
\emph off
\nospellcheck off
\bar no
\strikeout off
\xout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
x\pi_{k-1}=\pi_{k}+x\pi_{k-1}-\pi_{k}=\pi_{k}+r_{k-1}=\pi_{k}+\sum_{i=0}^{k-1}c_{i}\pi_{i},\quad r_{k-1}\in P_{k-1}
\]

\end_inset


\family default
\series default
\shape default
\size default
\emph default
\nospellcheck default
\bar default
\strikeout default
\xout default
\uuline default
\uwave default
\noun default
\color inherit

\begin_inset Formula 
\[
\langle\pi_{k},x\pi_{k-1}\rangle=\langle\pi_{k},\pi_{k}+\sum_{i=0}^{k-1}c_{i}\pi_{i}\rangle=\left\langle \pi_{k},\pi_{k}\right\rangle +\sum_{i=0}^{k-1}c_{i}\left\langle \pi_{k},\pi_{i}\right\rangle =\langle\pi_{k},\pi_{k}\rangle
\]

\end_inset


\end_layout

